/* ==UserStyle==
@name         Blockland Forum - 4Kidschan
@namespace    blf-4kidschan
@author       dargereldren
@description  BLF April Fools 2015 theme.
@version      1.0.1
@homepageURL  https://github.com/dargereldren/userstyles/tree/master/Blockland%20Forum%20-%204Kidschan
@preprocessor stylus
@var select theme 'Theme' ["yotsuba:Yotsuba", "yotsubab:Yotsuba B"]
@var select logo 'Logo' ["4kidschan:4Kidschan", "rainbowblf:4Kidschan-like BLF logo", "recolor:Red BLF logo", "default:Regular BLF logo"]
==/UserStyle== */

@-moz-document domain("forum.blockland.us") {
    if theme == 'yotsuba' {
		/* Normal, standard links. */
		a:link, a:visited
		{
			color: #800 !important;
		}
		a:hover
		{
			color: #E00 !important;
			text-decoration: underline !important;
		}

		/* By default (td, body..) use Verdana in black. */
		body 
		{
		background:url("https://forum.blockland.us/Themes/4Kids/images/fade.png") repeat-x scroll center top #FFE;
		}

		/* Alternating backgrounds... */
		.windowbg
		{
			background-color: #f0e0d6 !important; 
		}

		/* Titles - such as table headers. */
		.titlebg, tr.titlebg th, tr.titlebg td, .titlebg a:link, .titlebg a:visited, .titlebg2, tr.titlebg2 th, tr.titlebg2 td, .titlebg2 a:link, .titlebg2 a:visited
		{
			color: #ffffff !important;
			background-color: #DC1105 !important;
		}

		/* The category headers, page indexes, and such things. */
		.catbg
		{
			background-color: #880000 !important;
			background-image: none !important;
		}

		.catbg a:visited, .catbg a:link
		{
			color: #FFF !important;
		}

		.catbg a:hover
		{
			color: #CCF !important;
		}
    } else if theme == 'yotsubab' {
		/* Normal, standard links. */
		a:link, a:visited
		{
			color: #34345C !important;
		}
		a:hover
		{
			color: #E00 !important;
			text-decoration: underline !important;
		}

		/* By default (td, body..) use Verdana in black. */
		body 
		{
		background:url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAADICAIAAACmkByiAAAASUlEQVR4AcWRuQ0AIBDDzuy/HAVrMAM9slCorqAJziNgrj2qSg/cGhHnjPqDDPxOfYiebwFj+XobeLGI7p39fW1/Ib58d55Bwh3x9wRv6r75UwAAAABJRU5ErkJggg==") repeat-x scroll center top #EEF2FF;
		}

		/* Alternating backgrounds... */
		.windowbg
		{
			background-color: #D6DAF0 !important; 
		}

		/* Titles - such as table headers. */
		.titlebg, tr.titlebg th, tr.titlebg td, .titlebg a:link, .titlebg a:visited, .titlebg2, tr.titlebg2 th, tr.titlebg2 td, .titlebg2 a:link, .titlebg2 a:visited
		{
			color: #ffffff !important;
			background-color: #0511DC !important;
		}

		/* The category headers, page indexes, and such things. */
		.catbg
		{
			background-color: #000088 !important;
			background-image: none !important;
		}

		.catbg a:visited, .catbg a:link
		{
			color: #FFF !important;
		}

		.catbg a:hover
		{
			color: #FCC !important;
		}
    }
	body>table.tborder>tbody>tr,
	body>table:not(.tborder)>tbody>tr,
	body>table:not(.tborder)>tbody>tr>td {
		background-color: transparent !important;
	}
	
    if logo == '4kidschan' {
		.logoLink {
			width: 0 !important;
			height: 50px !important;
			padding-right: 400px !important;
			background-image: url("https://forum.blockland.us/Themes/4Kids/images/smflogo.png");
		}
    } else if logo == 'rainbowblf' {
		.logoLink {
			width: 0 !important;
			height: 50px !important;
			padding-right: 400px !important;
			background-image: url("data:image/png;base64,");
		}
    } else if logo == 'recolor' {
		.logoLink {
			width: 0 !important;
			height: 50px !important;
			padding-right: 400px !important;
			background-image: url("data:image/png;base64,");
		}
    } else if logo == 'default' {
		
	}
}
